msgid "<H2>Busiest day of the Week</H2>\n"
msgstr "<H2>Día más ocupado de la semana</H2>\n"

#: logintop10.c:356 logintop10.c:412
#, c-format
msgid "%s (%ld logins)\n"
msgstr "%s (%ld accesos)\n"

#: logintop10.c:357
#, c-format
msgid "<LI>Most quiet day: %s (%ld logins)\n"
msgstr "<LI>Día más tranquilo: %s (%ld accesos)\n"

#: logintop10.c:360 logintop10.c:388
msgid "<TR><TD><B>Day</B></TD><TD><B>Number of logins</B></TD></TR>\n"
