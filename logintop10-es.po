#: logintop10.c:181
msgid "<H1>Login/logout statistics</H1>\n"
msgstr "<H1>Estadísticas de acceso/desconexión</H1>\n"

#: logintop10.c:183
msgid "<H2>Overall data</H2>\n"
msgstr "<H2>Datos totales</H2>\n"

#: logintop10.c:185
#, c-format
msgid "<LI>Number of records: %d\n"
msgstr "<LI>Número de registros: %d\n"

#: logintop10.c:315
#, c-format
msgid "<LI>Min. duration: %ld\n"
msgstr "<LI>Duración Mín.: %ld\n"

#: logintop10.c:316
#, c-format
msgid "<LI>Max. duration: %ld\n"
msgstr "<LI>Duración Máx.: %ld\n"

#: logintop10.c:317
#, c-format
msgid "<LI>Average duration: %ld\n"
msgstr "<LI>Duración Media: %ld\n"

#: logintop10.c:320
msgid "<H2>Top 10 Login Frequency</H2>\n"
msgstr "<H2>Top 10 - Frecuencia de acceso\n"

#: logintop10.c:322
msgid "<TR><TD></TD><TD><B>Username:</B></TD><TD><B>Count</B></TD></TR>\n"
msgstr "<TR><TD></TD><TD><B>Usuario:</B></TD><TD><B>Cantidad</B></TD></TR>\n"

